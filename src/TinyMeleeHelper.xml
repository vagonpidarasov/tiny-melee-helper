<Ui xmlns="http://www.blizzard.com/wow/ui/"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.blizzard.com/wow/ui/ ..\FrameXML\UI.xsd">

	<Script file="frame.lua" />

	<Frame name="HeroicStrikeFrame" parent="UIParent" hidden="false" frameStrata="LOW" toplevel="true" inherits="SecureFrameTemplate">
		<Scripts>
			<OnLoad>
                TMHResolveSpells()
                TMHResolveItems()
			</OnLoad>

			<OnUpdate>
				TMHHeroicStrike()
				TMHProceedSpellQueue()
			</OnUpdate>
		</Scripts>
	</Frame>
</Ui>
